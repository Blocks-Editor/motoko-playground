{
  "canisters": {
    "playground_assets": {
      "frontend": {
        "entrypoint": "src/index.js"
      },
      "source": [
        "src/assets/",
        "dist/playground_assets/"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    },
    "tungsten": {
      "providers": [
        "https://gw.dfinity.network"
      ],
      "type": "persistent"
    }
  },
  "version": 1
}
